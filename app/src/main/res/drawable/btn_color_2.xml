<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffe5f3ff" />
            <corners android:radius="@dimen/px200" />
            <stroke android:width="@dimen/px2" android:color="#ff7bbffe" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffa1d2ff" />
            <corners android:radius="@dimen/px200" />
            <stroke android:width="@dimen/px2" android:color="#ff7bbffe" />
        </shape>
    </item>
</selector>